{
    "manifest_version":2,
    "name": "ReWordify",
    "version": "1.0",

    "default_locale": "en",
    "description": "Rewordify will help you with your word dilemmas"

    "background": {
	"persistent": false
    },

    "content_scripts": [{
	"matches":"http://*/*",
	"js": ["reword.js"]
    }],
    
    "browser_action": {
	"default_popup": "popup.html",
	"default_title": "ReWordify"
    },
}
