{
    "manifest_version": 2,
    "name": "ReWordify",
    "version": "1.0",

    "description": "Rewordify will help you with your word dilemmas",

    "browser_action": {
	"default_popup": "popup.html",
	"default_title": "ReWordify",	
	"default_icon": "icon.png"
    },

    "icons": {
	"128": "icon.png"
    },
    
    "permissions": [
	"activeTab"
    ]
}
